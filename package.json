{
  "name": "three-game-engine",
  "version": "0.6.0",
  "description": "Simple light-weight game engine using three.js, three-mesh-ui and rapier",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "lint": "eslint src/**",
    "lint:fix": "eslint src/** --fix",
    "build": "tsc --project tsconfig.json",
    "test": "jest",
    "prepublish": "npm run test && npm run build",
    "examples": "npm run build && node ./examples/examplesServer.js",
    "scene_editor": "npm run build && node ./scene_editor/sceneEditorServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WesUnwin/three-game-engine.git"
  },
  "keywords": [
    "three.js",
    "webgl",
    "game",
    "engine",
    "rapier",
    "three-mesh-ui"
  ],
  "author": "Wes Unwin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WesUnwin/three-game-engine/issues"
  },
  "homepage": "https://github.com/WesUnwin/three-game-engine#readme",
  "dependencies": {
    "@dimforge/rapier3d-compat": "^0.11.2",
    "events": "^3.3.0",
    "three": "^0.136.0",
    "three-mesh-ui": "^5.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.23.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.2",
    "@reduxjs/toolkit": "^1.9.7",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.51.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-contextmenu": "^2.14.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-redux": "^8.1.3",
    "react-slider": "^2.0.6",
    "react-tooltip": "^5.25.0",
    "style-loader": "^3.3.3",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-dev-server": "^4.15.1"
  }
}
